{
	"name": "chillerlan/php-cryptobox",
	"description": " A simple wrapper around the sodium cryptobox functions. PHP 7.2+",
	"homepage": "https://github.com/chillerlan/php-cryptobox",
	"license": "MIT",
	"type": "library",
	"minimum-stability": "stable",
	"keywords": [
		"crypto", "sodium", "php7"
	],
	"authors": [
		{
			"name": "Smiley",
			"email": "smiley@chillerlan.net",
			"homepage": "https://github.com/codemasher"
		}
	],
	"support": {
		"issues": "https://github.com/chillerlan/php-cryptobox/issues",
		"source": "https://github.com/chillerlan/php-cryptobox"
	},
	"require": {
		"php": "^7.2",
		"ext-sodium": "*"
	},
	"require-dev": {
		"phpunit/phpunit": "^8.4"
	},
	"autoload": {
		"psr-4": {
			"chillerlan\\Cryptobox\\": "src/"
		}
	},
	"autoload-dev": {
		"psr-4": {
			"chillerlan\\CryptoboxTest\\": "tests/"
		}
	}
}
